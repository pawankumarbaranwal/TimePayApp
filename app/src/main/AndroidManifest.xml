<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timepay" >


    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk android:minSdkVersion="16"
        android:targetSdkVersion="21"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomePage"
            android:label="HomePage"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Webview"
            android:label="WebView"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".PrivilageVendorRegistration"
            android:label="PrivilageVendorRegistration"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".Accounts"
            android:label="@string/title_activity_accounts"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".ChooseAccountType"
            android:label="@string/title_activity_choose_account_type" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".GeneralPublicRegistration"
            android:label="@string/title_activity_general_public_registration"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".VendorRegistration"
            android:label="@string/title_activity_vender_registration"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
